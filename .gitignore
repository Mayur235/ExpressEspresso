# Laravel folders to ignore
/vendor
/node_modules
/storage/*.key
/storage/oauth-private.key
/storage/oauth-public.key
.env
.env.backup
.phpunit.result.cache
Homestead.json
Homestead.yaml
auth.json
composer.lock
npm-debug.log
yarn-error.log

# IDE/Editor specific
/.idea
/.vscode

# OS-generated files
.DS_Store
Thumbs.db

# Logs and cache
/storage/logs/*
/bootstrap/cache/*

# Exceptions to keep .gitignore placeholders
!/storage/app/public
!/storage/app/public/**
!/storage/logs/.gitignore
!/bootstrap/cache/.gitignore

# Allow symlink to be tracked
!public/storage

# Docker-specific
docker-compose.override.yml
Dockerfile.*
